<%= form_for :contributors, url: contributors_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :age %><br>
    <%= f.text_field :age %>
  </p>
 
   <p>
    <%= f.label :birthplace  %><br>
    <%= f.text_field :birthplace %>
  </p>

  <p>
    <%= f.label :trivia  %><br>
    <%= f.text_area :trivia %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>